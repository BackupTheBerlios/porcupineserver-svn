<config>
	<!--_______________________________PACKAGES_SECTION________________________________-->
	
	<packages>
	<!-- insert here your own registrations. Leave SYSTEM_REGISTRATIONS as is -->
		<package name="parallax">
			<!-- XMLRPC -->
			<reg cc="org.innoscript.desktop.schema.common.RootFolder"
				method="POST"
				param="timeline_data"
				client="vcXMLRPC"
				lang=".*"
				action="com.blymee.parallax.XMLRPC.Timeline"/>
			<reg cc="Folders?$"
				method="POST"
				param="parallax"
				client="vcXMLRPC|MIDP|UNTRUSTED"
				lang=".*"
				action="com.blymee.parallax.XMLRPC.ContainerGeneric"/>
		</package>
		<package name="commanager">
		</package>
		<package name="metaface">
			<!-- metaface desktop -->
			<reg cc="org.innoscript.desktop.schema.common.RootFolder"
				method="GET"
				param=""
				client="(MSIE [6-7])|(Mozilla/5.0.+rv:1.[7-9])"
				lang=".*"
				action="com.blymee.metaface.ui.Desktop">
					<filter
						type="porcupine.filters.postProcessing.multilingual.Multilingual"
						using="org.innoscript.desktop.strings.resources"/>
			</reg>
			<!-- media forms -->
			<reg cc="Folder$"
				method="GET"
				param="new"
				qs="cc=com.blymee.metaface.schema"
				client="(MSIE [6-7])|(Mozilla/5.0.+rv:1.[7-9])"
				lang=".*"
				action="com.blymee.metaface.ui.Frm_NewMedia"/>
			<reg cc="com.blymee.metaface.schema.Video"
				method="GET"
				param="properties"
				client="(MSIE [6-7])|(Mozilla/5.0.+rv:1.[7-9])"
				lang=".*"
				action="com.blymee.metaface.ui.Frm_VideoProperties"/>
			<reg cc="com.blymee.metaface.schema.Image"
				method="GET"
				param="properties"
				client="(MSIE [6-7])|(Mozilla/5.0.+rv:1.[7-9])"
				lang=".*"
				action="com.blymee.metaface.ui.Frm_ImageProperties"/>
			
			<!-- media manipulation -->
			<reg cc="com.blymee.metaface.schema.(Video|Image)"
				method="GET"
				param="download"
				client=".*"
				lang=".*"
				action="com/blymee/metaface/download_media.psp"/>
			<reg cc="com.blymee.metaface.schema.Image"
				method="GET"
				param="thumbnail"
				client=".*"
				lang=".*"
				max-age="1200"
				action="com/blymee/metaface/image_thumbnail.jpg.psp"/>		
			<reg cc="com.blymee.metaface.schema.Video"
				method="GET"
				param="thumbnail"
				client=".*"
				lang=".*"
				max-age="1200"
				action="com/blymee/metaface/video_thumbnail.jpg.psp"/>

			<!-- XMLRPC -->
			<reg cc="Folders?$"
				method="POST"
				param="metaface"
				client="vcXMLRPC|MIDP|UNTRUSTED"
				lang=".*"
				action="com.blymee.metaface.XMLRPC.ContainerGeneric"/>
			<reg cc="RootFolder$"
				method="POST"
				param="metaface_facebook"
				client="vcXMLRPC"
				lang=".*"
				action="com.blymee.metaface.facebook.XMLRPC.Facebook"/>
		</package>
	</packages>
	
