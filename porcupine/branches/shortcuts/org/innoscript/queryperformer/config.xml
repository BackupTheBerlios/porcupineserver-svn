<config>
	<context match="(.*)\.quix"
		method="GET"
		client=".*"
		lang=".*"
		action="$1.quix"/>
	<context path="queryperformer.js"
		method="GET"
		client=".*"
		lang=".*"
		action="queryperformer.js"/>
	<!-- images -->
	<context match="images/(.*)"
		method="GET"
		client=".*"
		lang=".*"
		action="images/$1"
		max-age="3600"/>
</config>